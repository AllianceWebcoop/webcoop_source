<?xml version="1.0" encoding="utf-8"?>

<odoo>
  <record id="view_company_form_loan_consolidation" model="ir.ui.view">
    <field name="name">view_company_form_loan_consolidation</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
      <xpath expr="//notebook">
        <page name = "other_settings" string ="Other">
          <group name="other_accounting" string="Other">
            <field name="is_allow_restructure_again" />
          </group>
        </page>
      </xpath>
    </field>
  </record>

</odoo>
